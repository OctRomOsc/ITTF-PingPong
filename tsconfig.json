{
    "ts-node": {
    // It is faster to skip typechecking.
    // Remove if you want ts-node to do typechecking.
    //"transpileOnly": true,
    
    
    
    "swc": true,
  
    "files": true,
  
  },
    
    "compilerOptions": {
      "target": "es2016",                                 
      "module": "CommonJS",                                
      "declaration": true,                            
      "outDir": "dist",                                   
      "esModuleInterop": true,                             
      "forceConsistentCasingInFileNames": true,            
      "strict": true,                                    
      "skipLibCheck": true
                                      
    },
    "exclude": [
      "node_modules"
    ]
  }
  
  